{
  "name": "preact-f7",
  "amdName": "preactf7",
  "version": "1.2.1",
  "description": "Preact + Framework7",
  "main": "dist/preact-f7.js",
  "minified:main": "dist/preact-f7.min.js",
  "scripts": {
    "build": "npm run -s transpile && npm run -s minify && npm run -s size",
    "transpile": "rollup -c",
    "watch": "rollup -c --watch",
    "minify": "uglifyjs $npm_package_main -cm -o $npm_package_minified_main -p relative --in-source-map ${npm_package_main}.map --source-map ${npm_package_minified_main}.map",
    "size": "echo \"gzip size: $(gzip-size $npm_package_minified_main)\""
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread",
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "keywords": [
    "preact",
    "rich text",
    "input"
  ],
  "author": "Russian Rebou√ßas <russian.reboucas@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/russiann/preact-f7.git"
  },
  "bugs": {
    "url": "https://github.com/russiann/preact-f7/issues"
  },
  "homepage": "https://github.com/russiann/preact-f7",
  "peerDependencies": {
    "preact": "*"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-es2015-classes": "^6.3.15",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-react-jsx": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.3.13",
    "eslint": "^3.19.0",
    "framework7": "^2.0.0-beta.8",
    "gzip-size-cli": "^2.0.0",
    "jsx-control-statements": "^3.2.6",
    "mkdirp": "^0.5.1",
    "phantomjs-prebuilt": "^2.1.15",
    "postcss-discard-comments": "^2.0.2",
    "preact": "^8.2.1",
    "preact-jsx-chai": "^2.1.0",
    "pretty-bytes-cli": "^2.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.2.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-npm": "^2.0.0",
    "rollup-plugin-postcss": "^0.4.3",
    "rollup-watch": "^4.3.1",
    "uglify-js": "^2.8.24"
  }
}
